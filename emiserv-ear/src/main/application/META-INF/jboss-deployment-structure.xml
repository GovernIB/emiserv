<?xml version='1.0' encoding='UTF-8'?>
<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2">
	<deployment>
		<dependencies>
			<module name="org.keycloak.keycloak-core" export="TRUE"/>
		</dependencies>
	</deployment>
	<ear-subdeployments-isolated>false</ear-subdeployments-isolated>
	<sub-deployment name="emiservback.war">
		<exclude-subsystems>
			<subsystem name="jsf"/>
		</exclude-subsystems>
	</sub-deployment>
	<sub-deployment name="emiservapi-externa.war">
		<exclusions>
			<module name="emiserv.ear" />
		</exclusions>
		<exclude-subsystems>
			<subsystem name="jsf"/>
		</exclude-subsystems>
	</sub-deployment>
</jboss-deployment-structure>