			business-interface="es.caib.emiserv.core.api.service.ServeiService"/>
	<jee:local-slsb
			id="backofficeService"
			jndi-name="emiserv/BackofficeServiceBean/local"
			business-interface="es.caib.emiserv.core.api.service.BackofficeService"/>
	<jee:local-slsb
			id="redireccioService"
			jndi-name="emiserv/RedireccioServiceBean/local"
			business-interface="es.caib.emiserv.core.api.service.RedireccioService"/>
	<jee:local-slsb
			id="scspService"
			jndi-name="emiserv/ScspServiceBean/local"
